{
    "files.eol": "\n",
    "files.trimFinalNewlines": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
    },
    "eslint.enable": true,
    "eslint.alwaysShowStatus": true,
    "eslint.validate": [
        "typescript"
    ],
    "typescript.disableAutomaticTypeAcquisition": true,
    "editor.formatOnSave": true
}